pipeline {
    agent any
    stages {
        stage('checkout') {
            steps {
                git url: "https://github.com/parthshah0210/BasicWeb.git", branch: 'main'
                echo 'code checkout success'
            }
        }
        stage('Test') {
            steps {
                echo 'Running tests...'
            }
        }
        stage('Deploy') {
            steps {
                echo 'Deploying the project...'
            }
        }
    }
}
